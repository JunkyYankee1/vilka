# Database
DATABASE_URL=postgresql://kasashka:kasashka_password@postgres:5432/kasashka_db

# Redis
REDIS_URL=redis://redis:6379

# MinIO (S3-compatible storage)
MINIO_ENDPOINT=minio
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadminpassword
MINIO_BUCKET=media

# Next.js
NODE_ENV=development
NEXT_PUBLIC_API_BASE_URL=http://localhost

# Ollama (LLM inference)
OLLAMA_BASE_URL=http://ollama:11434
OLLAMA_MODEL=llama3.2:3b

# Yandex Maps (optional - for address autocomplete)
# NEXT_PUBLIC_YANDEX_MAPS_API_KEY=your_api_key_here

# Zabbix (optional - for monitoring)
# TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
